{
  "name": "@trapezedev/kotlin-parser",
  "version": "3.0.7",
  "description": "Parser Kotlin and Kotlin script programs",
  "author": "Ionic Team <hi@ionic.io> (https://ionicframework.com) ",
  "license": "SEE LICENSE",
  "private": false,
  "bin": "./bin/parser-kotlin",
  "type": "module",
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "clean": "rimraf ./dist",
    "build": "npm run clean && tsc",
    "watch": "tsc -w",
    "test": "jest"
  },
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "dependencies": {
    "@ionic/utils-fs": "^3.1.5",
    "@ionic/utils-subprocess": "^2.1.8",
    "@types/antlr4": "^4.7.2",
    "antlr4": "^4.10.1",
    "conventional-changelog": "^3.1.4",
    "cross-spawn": "^7.0.3",
    "env-paths": "^3.0.0",
    "ini": "^2.0.0",
    "lodash": "^4.17.21",
    "npm-watch": "^0.9.0",
    "ts-node": "^10.2.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ionic-team/trapeze.git"
  },
  "jest": {
    "preset": "ts-jest",
    "testTimeout": 30000,
    "transform": {
      "antlr4/.+\\.(j|t)sx?$": "ts-jest"
    },
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "mjs"
    ],
    "testMatch": [
      "**/test/**/*.test.(ts|tsx|js|mjs)"
    ]
  },
  "devDependencies": {
    "@types/cross-spawn": "^6.0.2",
    "@types/fs-extra": "^9.0.13",
    "@types/ini": "^1.3.31",
    "@types/jest": "^27.0.2",
    "@types/lodash": "^4.14.175",
    "@types/plist": "^3.0.2",
    "@types/slice-ansi": "^5.0.0",
    "jest": "^27.2.5",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.0.7",
    "typescript": "^4.4.4"
  },
  "gitHead": "3b4f9f02d6a74247a5606fe1c0688c9a1ec3fd4b"
}
